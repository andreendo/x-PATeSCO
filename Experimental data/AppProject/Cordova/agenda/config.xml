<?xml version='1.0' encoding='utf-8'?>
<widget id="app.agenda" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Agenda</name>
    <description>
        Agenda
    </description>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="false" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-installLocation" value="auto" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="StatusBarBackgroundColor" value="#3A4C54" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="www/icon.png" />
    <splash src="www/splash.png" />
    <icon height="29" platform="ios" src="www/res/icons/ios/icon-small.png" width="29" />
    <icon height="58" platform="ios" src="www/res/icons/ios/icon-small-2x.png" width="58" />
    <icon height="40" platform="ios" src="www/res/icons/ios/icon-40.png" width="40" />
    <icon height="80" platform="ios" src="www/res/icons/ios/icon-40-2x.png" width="80" />
    <icon height="50" platform="ios" src="www/res/icons/ios/icon-50.png" width="50" />
    <icon height="100" platform="ios" src="www/res/icons/ios/icon-50-2x.png" width="100" />
    <icon height="57" platform="ios" src="www/res/icons/ios/icon.png" width="57" />
    <icon height="114" platform="ios" src="www/res/icons/ios/icon-2x.png" width="114" />
    <icon height="60" platform="ios" src="www/res/icons/ios/icon-60.png" width="60" />
    <icon height="120" platform="ios" src="www/res/icons/ios/icon-60-2x.png" width="120" />
    <icon height="180" platform="ios" src="www/res/icons/ios/icon-60-3x.png" width="180" />
    <icon height="72" platform="ios" src="www/res/icons/ios/icon-72.png" width="72" />
    <icon height="144" platform="ios" src="www/res/icons/ios/icon-72-2x.png" width="144" />
    <icon height="76" platform="ios" src="www/res/icons/ios/icon-76.png" width="76" />
    <icon height="152" platform="ios" src="www/res/icons/ios/icon-76-2x.png" width="152" />
    <splash height="480" platform="ios" src="www/res/screens/ios/screen-iphone-portrait.png" width="320" />
    <splash height="960" platform="ios" src="www/res/screens/ios/screen-iphone-portrait-2x.png" width="640" />
    <splash height="1136" platform="ios" src="www/res/screens/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <splash height="1334" platform="ios" src="www/res/screens/ios/screen-iphone-portrait-667h.png" width="750" />
    <splash height="2208" platform="ios" src="www/res/screens/ios/screen-iphone-portrait-736h.png" width="1242" />
    <splash height="1024" platform="ios" src="www/res/screens/ios/screen-ipad-portrait.png" width="768" />
    <splash height="2048" platform="ios" src="www/res/screens/ios/screen-ipad-portrait-2x.png" width="1536" />
    <icon platform="android" qualifier="ldpi" src="www/res/icons/android/icon-36-ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="www/res/icons/android/icon-48-mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="www/res/icons/android/icon-72-hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="www/res/icons/android/icon-96-xhdpi.png" />
    <icon platform="android" qualifier="xxhdpi" src="www/res/icons/android/icon-144-xxhdpi.png" />
    <icon platform="android" qualifier="xxxhdpi" src="www/res/icons/android/icon-192-xxxhdpi.png" />
    <splash platform="android" qualifier="ldpi" src="www/res/screens/android/screen-ldpi-portrait.png" />
    <splash platform="android" qualifier="mdpi" src="www/res/screens/android/screen-mdpi-portrait.png" />
    <splash platform="android" qualifier="hdpi" src="www/res/screens/android/screen-hdpi-portrait.png" />
    <splash platform="android" qualifier="xhdpi" src="www/res/screens/android/screen-xhdpi-portrait.png" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.4" />
    <plugin name="cordova-plugin-device" spec="~1.1.3" />
    <plugin name="cordova-plugin-device-motion" spec="~1.2.2" />
    <plugin name="cordova-plugin-device-orientation" spec="~1.0.4" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.0" />
    <plugin name="cordova-plugin-file" spec="~4.3.0" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.6.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.5.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.0" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.0" />
    <plugin name="cordova-plugin-media" spec="~2.4.0" />
    <plugin name="cordova-plugin-camera" spec="~2.3.0">
        <variable name="CAMERA_USAGE_DESCRIPTION" value=" " />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value=" " />
    </plugin>
</widget>
